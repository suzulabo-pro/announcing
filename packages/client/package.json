{
  "name": "announcing-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "stencil build",
    "build.cap": "CAP_BUILD=y stencil build",
    "sync.android": "cap sync android && cp google-services.json android/app",
    "build.android": "npm run build.cap && npm run sync.android",
    "sync.ios": "cap sync ios && cp GoogleService-Info.plist ios/App/App && cp App.entitlements ios/App/App",
    "build.ios": "npm run build.cap && npm run sync.ios",
    "serve": "stencil build --dev --watch --serve --service-worker",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "start": "npm run serve"
  },
  "devDependencies": {
    "@announcing/shared": "*",
    "@announcing/shared-web": "*",
    "@announcing/shared-ui": "*",
    "@capacitor-community/http": "^1.1.2",
    "@capacitor/android": "^3.1.2",
    "@capacitor/app": "^1.0.2",
    "@capacitor/cli": "^3.1.2",
    "@capacitor/core": "^3.1.2",
    "@capacitor/ios": "^3.1.2",
    "@capacitor/local-notifications": "^1.0.3",
    "@capacitor/push-notifications": "^1.0.3",
    "@capacitor/share": "^1.0.3",
    "@capacitor/storage": "^1.0.3",
    "@rollup/plugin-replace": "^3.0.0",
    "@stencil/core": "^2.6.0",
    "@stencil/sass": "^1.4.1",
    "@stencil/store": "^1.4.1",
    "autolinker": "^3.14.3",
    "base-x": "^3.0.8",
    "bootstrap-icons": "^1.5.0",
    "cordova-res": "^0.15.3",
    "date-fns": "^2.23.0",
    "destyle.css": "^2.0.2",
    "firebase": "^9.0.0-beta.8",
    "idb": "^6.1.2",
    "pinch-zoom-element": "^1.1.1",
    "tweetnacl": "^1.0.3",
    "type-fest": "^1.2.2",
    "typescript": "^4.3.5",
    "workbox-build": "^4.3.1"
  }
}
