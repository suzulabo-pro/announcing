{
  "name": "announcing-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "stencil build",
    "build.cap": "CAP_BUILD=y stencil build",
    "sync.android": "cap sync android && cp google-services.json android/app",
    "build.android": "npm run build.cap && npm run sync.android",
    "sync.ios": "cap sync ios && cp GoogleService-Info.plist ios/App/App && cp App.entitlements ios/App/App",
    "build.ios": "npm run build.cap && npm run sync.ios",
    "serve": "stencil build --dev --watch --serve --service-worker",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "start": "npm run serve"
  },
  "devDependencies": {
    "@announcing/shared-ui": "*",
    "@capacitor-community/http": "^1.3.0",
    "@capacitor/android": "^3.2.2",
    "@capacitor/app": "^1.0.3",
    "@capacitor/cli": "^3.2.2",
    "@capacitor/core": "^3.2.2",
    "@capacitor/ios": "^3.2.2",
    "@capacitor/local-notifications": "^1.0.5",
    "@capacitor/push-notifications": "^1.0.4",
    "@capacitor/share": "^1.0.4",
    "@capacitor/storage": "^1.1.0",
    "@rollup/plugin-replace": "^3.0.0",
    "cordova-res": "^0.15.3",
    "idb": "^6.1.3",
    "pinch-zoom-element": "^1.1.1",
    "workbox-build": "^4.3.1"
  }
}
